{
    "name": "minutes.js",
    "version": "1.1.1",
    "description": "A library for working with minutes in JavaScript.",
    "main": "dist/minutes.js",
    "module": "src/minutes.js",
    "author": "Scott Mebberson",
    "scripts": {
        "pretest": "yarn rollup -c",
        "test": "yarn jest",
        "pretest-coverage": "yarn rollup -c",
        "test-coverage": "yarn jest --coverage"
    },
    "jest": {
        "transform": {
            "^.+\\.jsx?$": "babel-jest"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/smebberson/minutes.js.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "devDependencies": {
        "@babel/core": "7.7.7",
        "@babel/plugin-proposal-nullish-coalescing-operator": "7.7.4",
        "@babel/preset-env": "7.7.7",
        "@rollup/plugin-node-resolve": "7.0.0",
        "babel-jest": "24.9.0",
        "husky": "4.0.7",
        "istanbul": "0.4.3",
        "jest": "24.9.0",
        "prettier": "1.19.1",
        "pretty-quick": "2.0.1",
        "rollup": "1.29.0",
        "rollup-plugin-babel": "4.3.3"
    },
    "keywords": [
        "duration",
        "minutes",
        "time"
    ],
    "license": "MIT"
}
